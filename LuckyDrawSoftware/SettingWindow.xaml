            </TabItem>

            <TabItem Header="活动管理" Height="30"  Width="100">

            </TabItem>

            <TabItem Header="系统设置" Height="30"  Width="100">
                <Grid>
                    <view:SysSettingView/>
                </Grid>
            </TabItem>

        </TabControl>
    </Grid>
</Window>
